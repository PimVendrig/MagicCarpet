@model StationsResult

@{
    ViewData["Title"] = "NS Api";
}
<h2>@ViewData["Title"]</h2>
<h3>Stations</h3>

<div class="container">
    <div class="row">
        <div class="col-sm-6">
            Code
        </div>
        <div class="col-sm-6">
            Naam
        </div>
    </div>

    @foreach (Station station in Model.payload)
    {
    <div class="row">
        <div class="col-sm-6">
            @station.code
        </div>
        <div class="col-sm-6">
            @station.namen?.lang
        </div>
    </div>
    }
</div>

